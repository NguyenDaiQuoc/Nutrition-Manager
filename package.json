{
  "name": "nutrition-manager",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "npm run dev --workspace=apps/web",
    "dev:mobile": "npm run start --workspace=apps/mobile",
    "dev:all": "concurrently \"npm run dev:web\" \"npm run dev:mobile\"",
    "build:web": "npm run build --workspace=apps/web",
    "lint:web": "npm run lint --workspace=apps/web",
    "clean": "rimraf node_modules apps/*/node_modules packages/*/node_modules",
    "install:all": "npm install --workspaces"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native-stack": "^7.5.1",
    "expo-auth-session": "~7.0.8",
    "expo-camera": "~17.0.8",
    "expo-health-connect": "^0.1.1",
    "expo-web-browser": "~15.0.8",
    "firebase": "^12.4.0",
    "framer-motion": "^12.23.24",
    "lottie-react-native": "^5.1.4",
    "next": "^15.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-health": "^1.19.0",
    "react-native-progress": "^5.0.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-navigation": "^5.0.0",
    "undefined": "firebase/auth/react-native"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "concurrently": "^9.2.1",
    "rimraf": "^5.0.5",
    "typescript": "~5.9.2"
  }
}
